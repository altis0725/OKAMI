# Ubuntu環境でのNext.js最適化設定
legacy-peer-deps=true
fund=false
audit=false
progress=true
loglevel=verbose

# パッケージロック設定
package-lock=true
save-exact=false

# ネットワーク設定
fetch-retry-mintimeout=20000
fetch-retry-maxtimeout=120000
fetch-timeout=600000

# キャッシュ設定
cache-min=10

# Node.js環境設定
engine-strict=false
