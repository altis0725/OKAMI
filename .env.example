# API Keys
MONICA_API_KEY=sk-xxxxxxxxxxxxxxxxxxxxxxxx
OPENAI_API_BASE=https://openapi.monica.im/v1

# Optional: Mem0 API key for advanced memory features
MEM0_API_KEY=m0-xxxxxxxxxxxxxxxxxxxxxxxx

# Logging
OKAMI_LOG_LEVEL=INFO

# Storage paths (optional, defaults to ./storage)
CREWAI_STORAGE_DIR=./storage

# Claude Code Monitor webhook (optional)
CLAUDE_MONITOR_WEBHOOK_URL=

# Vector store settings
VECTOR_STORE_TYPE=qdrant
QDRANT_HOST=qdrant
QDRANT_PORT=6333
QDRANT_GRPC_PORT=6334

# Prometheus settings
PROMETHEUS_HOST=prometheus
PROMETHEUS_PORT=9090